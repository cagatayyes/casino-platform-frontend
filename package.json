{
	"name": "casino-platform-frontend",
	"version": "1.0.1",
	"private": true,
	"sideEffects": false,
	"dependencies": {
		"@emotion/react": "^11.11.3",
		"@emotion/styled": "^11.11.0",
		"@types/postcss-js": "^4.0.4",
		"autoprefixer": "^10.4.16",
		"axios": "^1.6.2",
		"babel-jest": "^27.4.2",
		"babel-loader": "^8.2.3",
		"babel-plugin-named-asset-import": "^0.3.8",
		"babel-preset-react-app": "^10.0.1",
		"bfj": "^7.0.2",
		"browserslist": "^4.18.1",
		"camelcase": "^6.2.1",
		"case-sensitive-paths-webpack-plugin": "^2.4.0",
		"classnames": "^2.5.1",
		"css-loader": "^6.5.1",
		"css-minimizer-webpack-plugin": "^3.2.0",
		"dayjs": "^1.11.10",
		"dotenv": "^16.3.2",
		"dotenv-expand": "^5.1.0",
		"file-loader": "^6.2.0",
		"fs-extra": "^10.0.0",
		"i18next": "^23.7.9",
		"i18next-browser-languagedetector": "^7.2.0",
		"i18next-http-backend": "^2.4.2",
		"identity-obj-proxy": "^3.0.0",
		"postcss": "^8.4.4",
		"postcss-cli": "^11.0.0",
		"postcss-flexbugs-fixes": "^5.0.2",
		"postcss-js": "^4.0.1",
		"postcss-loader": "^6.2.1",
		"postcss-normalize": "^10.0.1",
		"postcss-preset-env": "^7.0.1",
		"process": "^0.11.10",
		"prompts": "^2.4.2",
		"react": "^18.2.0",
		"react-app-polyfill": "^3.0.0",
		"react-dev-utils": "^12.0.1",
		"react-dom": "^18.2.0",
		"react-i18next": "^13.5.0",
		"react-refresh": "^0.11.0",
		"remove": "^0.1.5",
		"resolve": "^1.20.0",
		"resolve-url-loader": "^4.0.0",
		"sass": "^1.69.7",
		"sass-loader": "^13.3.2",
		"semver": "^7.3.5",
		"slugify": "^1.6.6",
		"source-map-loader": "^3.0.0",
		"style-loader": "^3.3.1",
		"svg-inline-loader": "^0.8.2",
		"svg-url-loader": "^8.0.0",
		"swiper": "^11.0.5",
		"tailwindcss": "^3.0.2",
		"terser-webpack-plugin": "^5.3.10",
		"url-loader": "^4.1.1"
	},
	"scripts": {
		"start": "env-cmd -f ./env/.env.development yarn run start:internal",
		"start:prod": "env-cmd -f ./env/.env.production yarn run start:internal",
		"start:internal": "node scripts/start.js",
		"build:internal": "node scripts/build.js",
		"test": "jest",
		"test:ci": "jest --watchAll=false",
		"prepare": "husky install",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
		"build": "env-cmd -f ./env/.env.production webpack",
		"build:dev": "env-cmd -f ./env/.env.development webpack --mode development",
		"build:test": "env-cmd -f ./env/.env.test webpack --mode development",
		"build:prod": "env-cmd -f ./env/.env.production webpack --mode production",
		"analyze": "yarn build:prod && webpack-bundle-analyzer dist/bundle-stats.json",
		"serve": "set PORT=3001 && env-cmd -f ./env/.env.development webpack --mode development"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"jest": {
		"verbose": true,
		"roots": [
			"<rootDir>/src"
		],
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}",
			"!src/**/*.d.ts"
		],
		"setupFiles": [
			"react-app-polyfill/jsdom"
		],
		"setupFilesAfterEnv": [
			"<rootDir>/src/setupTests.ts"
		],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
			"<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
		],
		"testEnvironment": "jsdom",
		"transform": {
			"^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
			"^.+\\.module\\.(css|sass|scss)$"
		],
		"modulePathIgnorePatterns": [
			"<rootDir>/src/App.tsx"
		],
		"modulePaths": [],
		"moduleNameMapper": {
			"^react-native$": "react-native-web",
			"^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
			"^casino/(.*)$": "<rootDir>/src/packages/$1",
			"^swiper/react": "<rootDir>/node_modules/swiper/swiper-react.d.ts",
			"^swiper/modules": "<rootDir>/node_modules/swiper/swiper.d.ts",
			"^swiper/css": "<rootDir>/node_modules/swiper/swiper.css",
			"^axios$": "axios/dist/node/axios.cjs"
		},
		"moduleFileExtensions": [
			"web.js",
			"js",
			"web.ts",
			"ts",
			"web.tsx",
			"tsx",
			"json",
			"web.jsx",
			"jsx",
			"node"
		],
		"watchPlugins": [
			"jest-watch-typeahead/filename",
			"jest-watch-typeahead/testname"
		],
		"resetMocks": true
	},
	"devDependencies": {
		"@babel/core": "^7.16.0",
		"@babel/preset-env": "^7.23.6",
		"@babel/preset-react": "^7.23.3",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
		"@svgr/webpack": "^5.5.0",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"@types/babel__core": "^7.20.5",
		"@types/i18next": "^13.0.0",
		"@types/jest": "^27.5.2",
		"@types/node": "^16.18.68",
		"@types/react": "^18.2.42",
		"@types/react-dom": "^18.2.17",
		"@types/react-i18next": "^8.1.0",
		"@types/react-router-dom": "^5.3.3",
		"@typescript-eslint/eslint-plugin": "^6.19.0",
		"@typescript-eslint/parser": "^6.13.2",
		"babel-plugin-module-resolver": "^5.0.0",
		"compression-webpack-plugin": "^10.0.0",
		"copy-webpack-plugin": "^12.0.1",
		"dotenv-webpack": "^8.0.1",
		"env-cmd": "^10.1.0",
		"eslint": "^8.3.0",
		"eslint-config-react-app": "^7.0.1",
		"eslint-config-standard": "^17.1.0",
		"eslint-config-standard-with-typescript": "^42.0.0",
		"eslint-plugin-jest": "^27.6.0",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-require-explicit-generics": "^1.0.0",
		"eslint-plugin-unused-imports": "^3.0.0",
		"eslint-webpack-plugin": "^3.1.1",
		"html-webpack-plugin": "^5.5.4",
		"husky": "^8.0.0",
		"jest": "^27.4.3",
		"jest-resolve": "^27.4.2",
		"jest-watch-typeahead": "^1.0.0",
		"mini-css-extract-plugin": "^2.7.7",
		"react-router-dom": "^6.3.0",
		"tailwind-styled-components": "^2.2.0",
		"ts-loader": "^9.5.1",
		"tsconfig-paths-webpack-plugin": "^4.2.0",
		"typescript": "^5.3.3",
		"web-vitals": "^2.1.4",
		"webpack": "^5.64.4",
		"webpack-bundle-analyzer": "^4.10.1",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^4.6.0",
		"webpack-manifest-plugin": "^4.0.2",
		"workbox-webpack-plugin": "^6.4.1"
	}
}
